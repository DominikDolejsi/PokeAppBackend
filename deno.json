{
  "deno": "1.46.2",
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^16.1.0",
    "@std/fmt": "jsr:@std/fmt@^1.0.1",
    "@zod/zod": "https://deno.land/x/zod@v3.23.8/mod.ts",
    "mongoose": "npm:mongoose@^8.6.0"
  },
  "tasks": {
    "dev": "deno -NESR --watch --env main.ts",
    "start": "deno -NESR main.ts",
    "compile": "deno compile -NESR --target x86_64-unknown-linux-gnu --output server main.ts",
    "code:check": "deno fmt --check && deno lint && deno check *.ts",
    "hooks:install": "deno run --allow-read=deno.json,.git/hooks/ --allow-write=.git/hooks/ jsr:@hongminhee/deno-task-hooks",
    "hooks:pre-commit": "deno task code:check"
  }
}
